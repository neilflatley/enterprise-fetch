{
  "name": "enterprise-fetch",
  "version": "1.0.0-alpha.5",
  "description": "A fetch wrapper to leverage enterprise features such as retry policy and request timeout",
  "browser": "dist/client/enterprise-fetch.esm.min.js",
  "module": "dist/enterprise-fetch.esm.js",
  "main": "dist/enterprise-fetch.cjs.js",
  "types": "dist/enterprise-fetch.d.ts",
  "scripts": {
    "build": "npm run clean && tsc && rollup -c && npm run instrument",
    "build:serve": "npm run build && npm run serve",
    "clean": "rimraf dist",
    "cy:open": "start-server-and-test serve http://localhost:5000 cypress:open",
    "cy:run": "cypress run",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "instrument": "nyc instrument --compact=false dist/enterprise-fetch.esm.js instrumented",
    "mocha:run": "mocha --es-module-specifier-resolution=node --require @babel/register --timeout 500000 --exit cypress/integration/node.spec.js",
    "nyc:test": "nyc npm test",
    "prestart": "npm run build",
    "report:coverage": "nyc report --reporter=html",
    "report:coverage:text": "nyc report --reporter=text",
    "report:coverage:summary": "nyc report --reporter=text-summary",
    "report-coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "start": "npm test",
    "serve": "serve .",
    "test": "start-server-and-test serve http://localhost:5000 cypress:run"
  },
  "keywords": [
    "fetch",
    "retry",
    "exponential",
    "backoff",
    "backoff",
    "factor",
    "timeout",
    "abort",
    "policy"
  ],
  "author": "Neil Flatley",
  "license": "ISC",
  "repository": "https://github.com/neilflatley/enterprise-fetch",
  "dependencies": {
    "abort-controller": "^3.0.0",
    "await-to-js": "^2.1.1",
    "cross-fetch": "^3.0.6",
    "https-proxy-agent": "^5.0.0",
    "promise-retry": "^2.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-export-default-from": "^7.12.13",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/plugin-syntax-export-default-from": "^7.12.13",
    "@babel/plugin-transform-classes": "^7.13.0",
    "@babel/plugin-transform-regenerator": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/register": "^7.13.8",
    "@cypress/code-coverage": "^3.9.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@types/node": "^14.14.33",
    "@types/node-fetch": "^2.5.8",
    "@types/promise-retry": "^1.1.3",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "chai": "^4.3.4",
    "cross-env": "^7.0.3",
    "cypress": "^6.6.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "mocha": "^8.3.2",
    "node-fetch": "^2.6.1",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.41.2",
    "rollup-plugin-terser": "^7.0.2",
    "serve": "^11.3.2",
    "start-server-and-test": "^1.12.0",
    "tslint": "^6.1.3",
    "typescript": "^4.2.3"
  }
}
