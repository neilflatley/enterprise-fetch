{
  "name": "enterprise-fetch",
  "version": "2.0.0",
  "description": "A fetch wrapper to leverage enterprise features such as retry policy and request timeout",
  "browser": "dist/client/enterprise-fetch.esm.min.js",
  "module": "dist/enterprise-fetch.esm.js",
  "main": "dist/enterprise-fetch.cjs.js",
  "types": "dist/enterprise-fetch.d.ts",
  "scripts": {
    "build": "npm run clean && tsc && rollup -c && npm run instrument",
    "build:serve": "npm run build && npm run serve",
    "clean": "rimraf dist && rimraf instrumented && rimraf coverage && rimraf .nyc_output",
    "cy:open": "start-server-and-test serve http://localhost:3000 cypress:open",
    "cy:run": "cypress run",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "instrument": "nyc instrument --compact=false dist/enterprise-fetch.esm.js instrumented",
    "mocha:run": "mocha --timeout 500000 --exit cypress/integration/node.spec.mjs",
    "nyc:test": "nyc npm run mocha:run && npm test",
    "prestart": "npm run build",
    "report:coverage": "nyc report --reporter=html && serve -c ../serve.json ./coverage",
    "report:coverage:text": "nyc report --reporter=text",
    "report:coverage:summary": "nyc report --reporter=text-summary",
    "codecov:linux": "curl -Os https://uploader.codecov.io/latest/linux/codecov && chmod +x codecov",
    "codecov:macos": "curl -Os https://uploader.codecov.io/latest/macos/codecov && chmod +x codecov",
    "codecov:windows": "curl -Os https://uploader.codecov.io/latest/windows/codecov.exe",
    "report-coverage": "npm run codecov:windows && codecov -t 9f816f9c-8faa-4812-9bb2-5235c8c11468",
    "start": "npm run nyc:test",
    "serve": "serve",
    "test": "start-server-and-test serve http://localhost:3000 cypress:run"
  },
  "keywords": [
    "fetch",
    "retry",
    "exponential",
    "backoff",
    "backoff",
    "factor",
    "timeout",
    "abort",
    "policy"
  ],
  "author": "Neil Flatley",
  "license": "ISC",
  "repository": "https://github.com/neilflatley/enterprise-fetch",
  "dependencies": {
    "abort-controller": "^3.0.0",
    "await-to-js": "^3.0.0",
    "https-proxy-agent": "^7.0.6",
    "node-fetch": "^2.7.0",
    "promise-retry": "^2.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/plugin-transform-runtime": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-typescript": "^7.27.1",
    "@babel/register": "^7.28.3",
    "@babel/runtime": "^7.28.4",
    "@codecov/rollup-plugin": "^1.9.1",
    "@cypress/code-coverage": "^3.14.7",
    "@eslint/js": "^9.38.0",
    "@rollup/plugin-babel": "^6.1.0",
    "@rollup/plugin-commonjs": "^28.0.8",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/node": "^22.9.0",
    "@types/node-fetch": "^2.6.13",
    "@types/promise-retry": "^1.1.6",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "chai": "^6.2.0",
    "cross-env": "^10.1.0",
    "cypress": "^15.5.0",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-chai-friendly": "^1.1.0",
    "eslint-plugin-cypress": "^5.2.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-mocha": "^11.2.0",
    "eslint-plugin-prettier": "^5.5.4",
    "fetch-mock": "^12.5.4",
    "mocha": "^11.7.4",
    "nyc": "^15.1.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "rollup": "^4.52.5",
    "serve": "^14.2.5",
    "start-server-and-test": "^2.1.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1",
    "url": "^0.11.4"
  }
}
